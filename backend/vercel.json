{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "1mb",
        "runtime": "python3.9",
        "excludeFiles": ["*"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/run.py"
    }
  ]
}