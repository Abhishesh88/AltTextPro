{
  "version": 2,
  "builds": [
    {
      "src": "run.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "5mb",
        "runtime": "python3.9",
        "excludeFiles": ["tests/**", "**/*.pyc", "**/__pycache__/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "run.py"
    }
  ],
  "functions": {
    "run.py": {
      "memory": 512,
      "maxDuration": 5
    }
  }
}